<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.train.dao.UserDao">
    <insert id="insertUser" parameterType="com.train.entity.User">
        insert ignore into user (user_id,user_name,user_gender,user_login_id,user_login_password)
        values (#{userId}, #{userName},#{userLoginId},#{userLoginPassword})
    </insert>
    <select id="selectUserList">
        select * from user
    </select>
</mapper>